<orders>

	<order ref="actuator_toggle">
		<ai>
			<ETL>1.5</ETL>
		</ai>
		<params>
			<param id="actuator_id"/>
		</params>
		<message dest="robot_movement_actuators">
			<command>actuator_toggle</command>
			<params>
				<param id="actuator_id" type="positionxy"/>
			</params>
		</message>
	</order>


	<order ref="wheels_goto">
		<ai>
			<ETL>6</ETL>
		</ai>
		<params>
			<param id="position"/>
		</params>
		<message dest="robot_movement_wheels">
			<command>goto</command>
			<params>
				<param id="position"/>
			</params>
		</message>
	</order>


	<order ref="actuator_open">
		<ai>
			<ETL>1.5</ETL>
		</ai>
		<params>
			<param id="actuator_id"/>
		</params>
		<message dest="robot_movement_actuators">
			<command>actuator_open</command>
			<params>
				<param id="actuator_id"/>
			</params>
		</message>
	</order>
	<order ref="actuator_close">
		<ai>
			<ETL>1.5</ETL>
		</ai>
		<params>
			<param id="actuator_id"/>
		</params>
		<message dest="robot_movement_actuators">
			<command>actuator_close</command>
			<params>
				<param id="actuator_id"/>
			</params>
		</message>
	</order>

	<order ref="ai_delay">
		<ai>
			<ETL>2</ETL>
		</ai>
		<params>
			<param id="time_sec"/>
		</params>
		<message dest="robot_ai">
			<command>ai_delay</command>
			<params>
				<param id="time_sec"/>
			</params>
		</message>
	</order>

	<order ref="wait_for_jack">
		<ai>
			<ETL>20</ETL>
		</ai>
		<params></params>
		<message dest="robot_movement_actuators">
			<command>reply_when_jack</command>
			<params></params>
		</message>
	</order>

	<order ref="timer_start">
		<ai>
			<ETL>0</ETL>
		</ai>
		<params></params>
		<message dest="robot_ai">
			<command>timer_start</command>
			<params></params>
		</message>
	</order>

	<order ref="funny_action">
		<ai>
			<ETL>4</ETL>
		</ai>
		<params></params>
		<message dest="robot_movement_actuators">
			<command>funny_action</command>
			<params></params>
		</message>
	</order>

</orders>